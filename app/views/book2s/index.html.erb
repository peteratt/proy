<h1>Listing book2s</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @book2s.each do |book2| %>
  <tr>
    <td><%= book2.title %></td>
    <td><%= book2.description %></td>
    <td><%= book2.price %></td>
    <td><%= link_to 'Show', book2 %></td>
    <td><%= link_to 'Edit', edit_book2_path(book2) %></td>
    <td><%= link_to 'Destroy', book2, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Book2', new_book2_path %>
